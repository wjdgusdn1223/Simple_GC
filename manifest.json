{
  "name": "Add Schedule Simply",
    "version": "1.2",
    "description": "간단하게 일정을 추가할 수 있는 확장프로그램",
    "manifest_version": 2,
    "browser_action": {
      "default_title": "Simple_GC",
      "default_popup": "index.html"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "windows": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y",
          "chromeos": "Ctrl+Shift+U",
          "linux": "Ctrl+Shift+J"
        },
        "description": "Simple_GC shortcut key"
      }
    },
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    },
    "permissions": [
      "identity"
    ],
    "oauth2": {
      "client_id": "765290039571-q8jaqvtipe8pq67b5s7u0otiuohpmq3d.apps.googleusercontent.com",
      "scopes":[
        "https://www.googleapis.com/auth/calendar",
        "https://www.googleapis.com/auth/calendar.events"
      ]
    },
    "key": ""
  }
